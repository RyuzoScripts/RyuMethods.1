local usersLabel = Instance.new("TextLabel", frame)
usersLabel.Size = UDim2.new(1, 0, 0.15, 0)
usersLabel.Position = UDim2.new(0, 0, 0.88, 0)
usersLabel.BackgroundTransparency = 1
usersLabel.TextScaled = true
usersLabel.Font = Enum.Font.Gotham
usersLabel.TextColor3 = Color3.fromRGB(150, 255, 180)
usersLabel.Text = "Active Users: 2,154"

local function updateUsers()
	local count = 2100
	task.spawn(function()
		while count < 3000 do
			wait(math.random(2, 5))
			count += math.random(50, 150)
			usersLabel.Text = "Active Users: " .. tostring(count)
		end
	end)
end
updateUsers()

local logs = {
	"Initializing bypass core...",
	"Bypassing remote logs...",
	"Injecting packet filters...",
	"🌱 Egg reroll system initialized...",
	"Rerolling rare egg traits...",
	"Deploying garden hooks...",
	"Final handshake complete."
}

for i = 1, 100 do
	bar:TweenSize(UDim2.new(i / 100, 0, 1, 0), "Out", "Sine", 0.2, true)
	percent.Text = i .. "%"
	if i % 17 == 0 then
		status.Text = logs[math.min(math.ceil(i / 17), #logs)]
	end
	wait(0.25 + math.random() * 0.1)
end

-- Fade out after complete
TweenService:Create(frame, TweenInfo.new(1), {BackgroundTransparency = 1}):Play()
TweenService:Create(percent, TweenInfo.new(1), {TextTransparency = 1}):Play()
TweenService:Create(status, TweenInfo.new(1), {TextTransparency = 1}):Play()
TweenService:Create(title, TweenInfo.new(1), {TextTransparency = 1}):Play()
TweenService:Create(usersLabel, TweenInfo.new(1), {TextTransparency = 1}):Play()
TweenService:Create(bar, TweenInfo.new(1), {BackgroundTransparency = 1}):Play()
TweenService:Create(barBG, TweenInfo.new(1), {BackgroundTransparency = 1}):Play()
wait(1.2)
gui:Destroy()

