menuFrame.BackgroundTransparency = 1
Instance.new("UICorner", menuFrame).CornerRadius = UDim.new(0, 10)
TweenService:Create(menuFrame, TweenInfo.new(1), {BackgroundTransparency = 0.1}):Play()

-- Create buttons with animation
local function createButton(text, icon, order)
	local btn = Instance.new("TextButton", menuFrame)
	btn.Size = UDim2.new(0.9, 0, 0.35, 0)
	btn.Position = UDim2.new(0.05, 0, 0.1 + (order * 0.45), 0)
	btn.BackgroundColor3 = Color3.fromRGB(50, 100, 60)
	btn.Text = icon .. "  " .. text
	btn.TextScaled = true
	btn.Font = Enum.Font.GothamBold
	btn.TextColor3 = Color3.new(1, 1, 1)
	btn.AutoButtonColor = false
	Instance.new("UICorner", btn).CornerRadius = UDim.new(0, 6)

	btn.MouseEnter:Connect(function()
		TweenService:Create(btn, TweenInfo.new(0.2), {
			BackgroundColor3 = Color3.fromRGB(70, 130, 80)
		}):Play()
	end)
	btn.MouseLeave:Connect(function()
		TweenService:Create(btn, TweenInfo.new(0.2), {
			BackgroundColor3 = Color3.fromRGB(50, 100, 60)
		}):Play()
	end)

	btn.MouseButton1Click:Connect(function()
		print(text .. " button clicked!")
	end)
end

-- Two buttons
createButton("Egg Reroll", "🥚", 0)
createButton("Pet Sell Exploit", "🐾", 1)
