local menuFrame = Instance.new("Frame", gui2)
menuFrame.Size = UDim2.new(0.4, 0, 0.3, 0)
menuFrame.Position = UDim2.new(0.5, 0, 0.45, 0)
menuFrame.AnchorPoint = Vector2.new(0.5, 0.5)
